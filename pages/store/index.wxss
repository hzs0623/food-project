.onewrap {
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.twowarap {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.store-page {
  padding: 0 30rpx;
}
.store-page .header {
  padding: 20rpx 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.store-page .header span {
  padding: 0 40rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  color: #fff;
  border-radius: 32rpx;
  box-shadow: 1rpx 5rpx 5rpx #4e4c4c;
}
.store-page .title {
  margin-top: 20rpx;
  height: 14px;
  line-height: 14px;
  font-size: 14px;
  display: flex;
  align-items: center;
  font-weight: bold;
}
.store-page .title span {
  display: inline-block;
  width: 2px;
  height: 100%;
  background: #333;
  margin-right: 4px;
}
.store-page .t-swiper {
  border-radius: 32rpx;
}
.store-page .t-swiper image {
  border-radius: 32rpx;
}
page {
  background-color: #f5f5f5;
}
 
