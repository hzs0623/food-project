.onewrap {
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.twowarap {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.tab-comp {
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
}
.tab-comp .tab-item {
  height: 48rpx;
  padding: 40rpx;
  line-height: 48rpx;
  font-size: 12px;
  color: #333;
}
 
