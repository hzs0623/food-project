.onewrap {
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.twowarap {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.store-detail-page image {
  height: 200px;
  object-fit: cover;
}
.store-detail-page .wrap {
  padding: 0 20rpx;
}
.store-detail-page .wrap .common {
  padding: 20rpx;
  transform: translateY(-140px);
}
.store-detail-page .wrap .store-detail {
  padding: 20rpx;
  transform: translateY(-140px);
  background: #fff;
  border-radius: 32rpx;
}
.store-detail-page .wrap .store-detail .title {
  font-weight: bold;
}
.store-detail-page .wrap .store-detail .info {
  height: 14px;
  line-height: 14px;
  display: flex;
  align-items: center;
  font-size: 12px;
}
.store-detail-page .wrap .store-detail .info .rate {
  display: flex;
  align-items: center;
  color: #ffc51c;
  margin-right: 20px;
}
.store-detail-page .wrap .store-detail .t-swiper {
  margin-top: 8px;
  border-radius: 32rpx;
}
.store-detail-page .wrap .store-detail .t-swiper image {
  border-radius: 32rpx;
}
.store-detail-page .wrap .store-detail .site {
  margin-top: 8px;
  font-size: 12px;
  font-weight: bold;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.store-detail-page .wrap .store-detail .site span {
  font-weight: 400;
  color: #ccc;
}
.store-detail-page .wrap .commontitle {
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
  height: 14px;
  line-height: 14px;
  font-size: 14px;
}
.store-detail-page .wrap .commontitle span {
  display: inline-block;
  margin-right: 4px;
  height: 100%;
  width: 2px;
  background-color: #333;
}
.store-detail-page .wrap .coupon {
  margin-top: 10px;
  padding: 20rpx;
  transform: translateY(-140px);
}
.store-detail-page .wrap .coupon .coupon-item {
  padding: 20rpx;
  background-color: #fff;
  font-size: 12px;
  display: flex;
  justify-content: space-between;
}
.store-detail-page .wrap .coupon .coupon-item .coupon-item-content {
  line-height: 16px;
}
.store-detail-page .wrap .coupon .coupon-item .coupon-item-content .coupon-item-title {
  font-weight: bold;
}
.store-detail-page .wrap .coupon .coupon-item .coupon-item-content .coupon-explain {
  color: #ccc;
}
.store-detail-page .wrap .coupon .coupon-item .coupon-item-content .coupon-price {
  color: #fd6303;
  font-weight: bold;
}
.store-detail-page .wrap .coupon .coupon-item .coupon-item-right {
  display: flex;
  align-items: center;
}
.store-detail-page .wrap .coupon .coupon-item .coupon-btn {
  background-color: #fd6303;
  padding: 4px 10px;
  border-radius: 32rpx;
  color: #fff;
  font-size: 12px;
}
.store-detail-page .wrap .set-meal {
  margin-top: 10px;
  padding: 20rpx;
  transform: translateY(-140px);
}
page {
  background-color: #f5f5f5;
}
 
